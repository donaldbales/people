<p id="notice"><%= notice %></p>

<h1>Listing Industrial Hygiene Jobs</h1>

<table>
  <thead>
    <tr>
      <th>Industrial hygiene job type</th>
      <th>Parent</th>
      <th>Code</th>
      <th>Description</th>
      <th>Active datetime</th>
      <th>Inactive datetime</th>
      <th>Level</th>
      <th>Context</th>
      <th>Context code</th>
      <th>Context description</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @industrial_hygiene_jobs.each do |industrial_hygiene_job| %>
      <tr>
        <td><%= industrial_hygiene_job.industrial_hygiene_job_type_id %></td>
        <td><%= industrial_hygiene_job.parent_id %></td>
        <td><%= industrial_hygiene_job.code %></td>
        <td><%= industrial_hygiene_job.description %></td>
        <td><%= industrial_hygiene_job.active_datetime %></td>
        <td><%= industrial_hygiene_job.inactive_datetime %></td>
        <td><%= industrial_hygiene_job.level %></td>
        <td><%= industrial_hygiene_job.context_id %></td>
        <td><%= industrial_hygiene_job.context_code %></td>
        <td><%= industrial_hygiene_job.context_description %></td>
        <td><%= link_to 'Show', industrial_hygiene_job %></td>
        <td><%= link_to 'Edit', edit_industrial_hygiene_job_path(industrial_hygiene_job) %></td>
        <td><%= link_to 'Destroy', industrial_hygiene_job, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Industrial hygiene job', new_industrial_hygiene_job_path %>
