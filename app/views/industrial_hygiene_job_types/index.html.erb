<p id="notice"><%= notice %></p>

<h1>Listing Industrial Hygiene Job Types</h1>

<table>
  <thead>
    <tr>
      <th>Code</th>
      <th>Description</th>
      <th>Active datetime</th>
      <th>Inactive datetime</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @industrial_hygiene_job_types.each do |industrial_hygiene_job_type| %>
      <tr>
        <td><%= industrial_hygiene_job_type.code %></td>
        <td><%= industrial_hygiene_job_type.description %></td>
        <td><%= industrial_hygiene_job_type.active_datetime %></td>
        <td><%= industrial_hygiene_job_type.inactive_datetime %></td>
        <td><%= link_to 'Show', industrial_hygiene_job_type %></td>
        <td><%= link_to 'Edit', edit_industrial_hygiene_job_type_path(industrial_hygiene_job_type) %></td>
        <td><%= link_to 'Destroy', industrial_hygiene_job_type, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

