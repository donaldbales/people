<p id="notice"><%= notice %></p>

<h1>Listing Person Identifiers</h1>

<table>
  <thead>
    <tr>
      <th>Person</th>
      <th>Identifier</th>
      <th>Active datetime</th>
      <th>Inactive datetime</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @person_identifiers.each do |person_identifier| %>
      <tr>
        <td><%= person_identifier.person_id %></td>
        <td><%= person_identifier.identifier_id %></td>
        <td><%= person_identifier.active_datetime %></td>
        <td><%= person_identifier.inactive_datetime %></td>
        <td><%= link_to 'Show', person_identifier %></td>
        <td><%= link_to 'Edit', edit_person_identifier_path(person_identifier) %></td>
        <td><%= link_to 'Destroy', person_identifier, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Person identifier', new_person_identifier_path %>
