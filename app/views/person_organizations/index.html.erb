<p id="notice"><%= notice %></p>

<h1>Listing Person Organizations</h1>

<table>
  <thead>
    <tr>
      <th>Person</th>
      <th>Organization</th>
      <th>Active datetime</th>
      <th>Inactive datetime</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @person_organizations.each do |person_organization| %>
      <tr>
        <td><%= person_organization.person_id %></td>
        <td><%= person_organization.organization_id %></td>
        <td><%= person_organization.active_datetime %></td>
        <td><%= person_organization.inactive_datetime %></td>
        <td><%= link_to 'Show', person_organization %></td>
        <td><%= link_to 'Edit', edit_person_organization_path(person_organization) %></td>
        <td><%= link_to 'Destroy', person_organization, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Person organization', new_person_organization_path %>
