<p id="notice"><%= notice %></p>

<h1>Listing Positions</h1>

<table>
  <thead>
    <tr>
      <th>Position type</th>
      <th>Parent</th>
      <th>Code</th>
      <th>Description</th>
      <th>Active datetime</th>
      <th>Inactive datetime</th>
      <th>Level</th>
      <th>Context</th>
      <th>Context code</th>
      <th>Context description</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @positions.each do |position| %>
      <tr>
        <td><%= position.position_type_id %></td>
        <td><%= position.parent_id %></td>
        <td><%= position.code %></td>
        <td><%= position.description %></td>
        <td><%= position.active_datetime %></td>
        <td><%= position.inactive_datetime %></td>
        <td><%= position.level %></td>
        <td><%= position.context_id %></td>
        <td><%= position.context_code %></td>
        <td><%= position.context_description %></td>
        <td><%= link_to 'Show', position %></td>
        <td><%= link_to 'Edit', edit_position_path(position) %></td>
        <td><%= link_to 'Destroy', position, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Position', new_position_path %>
